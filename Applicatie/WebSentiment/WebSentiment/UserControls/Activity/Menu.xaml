<UserControl
    x:Class="WebSentiment.UserControls.Activity.Menu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="1000"
    d:DesignWidth="768">

    <Grid Background="{ThemeResource AppBackgroundWhite}">
        <Grid.RowDefinitions>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel x:Name="spRowOne" Grid.Row="0" />
        <StackPanel x:Name="spRowTwo" Grid.Row="1" />
        <StackPanel x:Name="spRowThree" Grid.Row="2" />
        <StackPanel x:Name="spRowFour" Grid.Row="3" />
        <StackPanel x:Name="spRowFive" Grid.Row="4" />
    </Grid>
</UserControl>
